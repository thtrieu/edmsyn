mean.n.2.vec <- function(x){
m <- x[[1]]
n <- x[[2]]
sapply(1:n, function(x){
i <- n-x+1
r <- runif(1,max(i*m-(i-1),0),min(i*m,1))
m <<- (i*m - r)/(i-1)
return(r)
})
}

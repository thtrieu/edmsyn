mean.n.2.vec <- function(x){
m <- x[[1]]
n <- x[[2]]
sapply(1:n, function(x){
i <- n-x+1
r <- runif(1,i*m-(i-1),i*m)
m <<- (i*m - r)/(i-1)
return(r)
})
}
